{
  "name": "Chrome Extension Webpack",
  "version": "0.1",
  "options_page": "options.html",
  "background": {
    "page": "background.html"
  },
  "content_scripts": [{
   "js": ["content.bundle.js"],
    "matches": ["https://*.upwork.com/*"]
  }],
  "icons": {
    "128": "icon-128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
